{
  "name": "react-typescript-example",
  "version": "0.1.0",
  "homepage": "https://inokawa.github.io/react-typescript-example/",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve --mode=development",
    "build": "webpack --mode=production",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "format:ci": "prettier '**/*.{js,jsx,ts,tsx}' --list-different",
    "tsc": "tsc -p . --noEmit",
    "screenshot": "storycap --serverCmd 'npm run storybook' http://localhost:6006 --serverTimeout 3000000",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/css": "11.9.0",
    "core-js": "3.21.0",
    "formik": "2.2.9",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "recoil": "^0.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-webpack5": "^6.5.3",
    "@storybook/manager-webpack5": "^6.5.3",
    "@storybook/react": "^6.5.3",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/react": "13.2.0",
    "@tsconfig/strictest": "^1.0.1",
    "@types/node": "17.0.35",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "prettier": "2.4.1",
    "storycap": "3.1.8",
    "style-loader": "3.3.1",
    "ts-jest": "28.0.3",
    "typescript": "4.5.5",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
